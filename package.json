  "author": "Alfian Rivaldi",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.js\" \"index.js\" \"test/**/*.js\"",
    "dev": "nodemon index",
    "test": "jest --forceExit",
