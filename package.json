  },
  "dependencies": {
    "@prisma/client": "^2.30.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "supertest": "^6.1.6",
    "vercel": "^23.1.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^2.30.0"
  }
}
